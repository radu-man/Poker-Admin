    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>aspnet-PokerAdmin-b8e00aba-3cab-4c71-ac4f-68464e81d7e9</UserSecretsId>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(RunConfiguration)' == 'https' " />
