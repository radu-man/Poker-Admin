@model IEnumerable<PokerAdmin.Models.Sesiune>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrasId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClubId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JocId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Jucator)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrasId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClubId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JocId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Jucator.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
